x=-4:0.1:4;
y=-4:0.1:4;
[x,y]=meshgrid(x,y);
z = cos(var*x)*(x^3)+(y^3);
mesh(x,y,z);
xlabel('Ox') 
ylabel('Oy') 
zlabel('Oz')
x=-4:0.1:4;
y=-4:0.1:4;
[x,y]=meshgrid(x,y);
z = cos(var*x)*(x*x*x)+(y*y*y);
mesh(x,y,z);
xlabel('Ox') 
ylabel('Oy') 
zlabel('Oz')
var=54
x=-4:0.1:4;
y=-4:0.1:4;
[x,y]=meshgrid(x,y);
z = cos(var*x)*(x^3)+(y^3);
mesh(x,y,z);
xlabel('Ox') 
ylabel('Oy') 
zlabel('Oz')
main
